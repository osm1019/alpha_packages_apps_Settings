<?xml version="1.0" encoding="utf-8" ?>

<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/alpha_color_ripple">
    <item android:id="@android:id/mask" android:width="34dp" android:height="34dp" android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="#FFFFFFFF" />
        </shape>
    </item>
    <item id="@android:id/background" android:width="34dp" android:height="34dp" android:gravity="center">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#1affffff" />
        </shape>
    </item>
    <item
        android:width="32dp"
        android:height="40dp"
        android:drawable="@drawable/ic_settings_search"
        android:gravity="center"
        android:left="2dp"
        android:bottom="2dp"
        android:right="2dp"
        android:top="2dp" />
</ripple>
